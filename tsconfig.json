{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es6",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "baseUrl": "./",
    "paths": {
      "Page/*": ["src/page/*"],
      "Routing/*": ["src/routing/*"],
      "Components/*": ["src/components/*"],
      "Store/*": ["src/store/*"],
      "Model/*": ["src/initialize-config/model/*"]
    }
  }
}
